{
  "name": "@uirouter/rx",
  "version": "0.4.4",
  "description": "Reactive extensions for UI-Router",
  "scripts": {
    "clean": "shx rm -rf lib lib-esm _bundles",
    "build": "npm run clean && tsc && tsc -outDir lib-esm -m es6",
    "package": "npm run build && rollup -c && rollup -c --environment MINIFY && npm run fixmaps:lib && npm run fixmaps:bundle",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fixmaps:lib": "tweak_sourcemap_paths -a --include 'lib/**/*.js.map' 'lib-esm/**/*.js.map'",
    "fixmaps:bundle": "tweak_sourcemap_paths -a --include '_bundles/**/*.js.map'",
    "release": "release",
    "prepublishOnly": "npm run package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-router/rx.git"
  },
  "keywords": [
    "ui-router",
    "reactive",
    "rxjs"
  ],
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "typings": "lib/index.d.ts",
  "author": "Chris Thielen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ui-router/rx/issues"
  },
  "homepage": "https://github.com/ui-router/rx#readme",
  "peerDependencies": {
    "@uirouter/core": ">=5.0.0",
    "rxjs": "^5.0.3"
  },
  "devDependencies": {
    "@uirouter/core": ">=5.0.0",
    "@uirouter/publish-scripts": "2.0.2",
    "conventional-changelog": "^1.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "readline-sync": "^1.4.5",
    "rxjs": "^5.0.3",
    "shx": "^0.2.2",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-progress": "^0.2.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^1.0.2",
    "rollup-plugin-visualizer": "^0.2.1"
  }
}
